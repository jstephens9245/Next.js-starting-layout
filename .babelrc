{
  "plugins": [
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "@styles": "./styles",
          "@components": "./components",
          "@pages": "./pages"
        }
      }
    ],
    [
      "wrap-in-js", {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel"
  ],
  "ignore": []
}
